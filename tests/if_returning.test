@extern("write")
write(f: i32, buf: str, len: usize): i32

true_msg  = "it's true!\n"
false_msg = "it's false\n"

get_msg(cond) =
    if cond then
        true_msg
    else
        false_msg

main: i32 =
    _ = write(1 get_msg(true) 11)
    _ = write(1 get_msg(false) 11)
    0
---
it's true!
it's false
